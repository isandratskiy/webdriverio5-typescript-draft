{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["./*"],
      "src/*": ["./src/*"],
      "test/*": ["./test/*"]
    },
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "types": ["@wdio/sync", "mocha", "node"],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
